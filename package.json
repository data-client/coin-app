{
  "name": "coinbase-lite",
  "version": "0.0.1",
  "packageManager": "yarn@3.6.3",
  "description": "coinbase-lite - An Anansi project",
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "format": "npm run lint --fix",
    "test:type": "tsc",
    "start": "anansi serve --dev ./src/index.tsx",
    "start:server": "anansi serve ./dist-server/App.js -a",
    "build": "webpack --mode=production",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "test:pkg": "webpack --env check=nobuild",
    "postinstall": "rm -rf node_modules/.cache",
    "build:server": "webpack --mode=production --target=node --env entrypath=index.server.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ntucker/coinbase-lite.git"
  },
  "bugs": {
    "url": "https://github.com/ntucker/coinbase-lite/issues"
  },
  "homepage": "https://github.com/ntucker/coinbase-lite#readme",
  "keywords": [
    "anansi"
  ],
  "devDependencies": {
    "@anansi/babel-preset": "*",
    "@anansi/browserslist-config": "*",
    "@anansi/eslint-plugin": "*",
    "@anansi/webpack-config": "*",
    "@babel/core": "*",
    "@data-client/test": "*",
    "@linaria/babel-preset": "*",
    "@linaria/core": "*",
    "@linaria/react": "*",
    "@linaria/shaker": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-test-renderer": "*",
    "@typescript-eslint/eslint-plugin": "^5.11.0 || ^6.0.0",
    "@typescript-eslint/parser": "*",
    "eslint": "^7.0.0 || ^8.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0 || ^5.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1 || ^3.0.0",
    "react-refresh": "*",
    "react-test-renderer": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "@anansi/cli": "*",
    "@anansi/core": "*",
    "@anansi/router": "*",
    "@babel/runtime": "*",
    "@data-client/img": "*",
    "@data-client/react": "*",
    "@data-client/redux": "*",
    "@data-client/rest": "*",
    "history": "*",
    "react": "*",
    "react-dom": "*",
    "redux": "*"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "MIT",
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "@linaria/babel-preset": "4.4.5",
    "@linaria/core": "4.2.10",
    "@linaria/webpack5-loader": "4.1.17",
    "@linaria/react": "4.3.8",
    "@linaria/shaker": "4.2.11",
    "@linaria/utils": "4.3.4",
    "@linaria/tags": "4.3.5"
  }
}
