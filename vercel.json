{
  "installCommand": "yarn install",
  "buildCommand": "yarn build && yarn build:server",
  "devCommand": "yarn start",
  "outputDirectory": "dist",
  "functions": {
    "api/*.mjs": {
      "includeFiles": "dist-server/*",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "cleanUrls": true
}
